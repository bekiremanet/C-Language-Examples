#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <ctype.h>
#include <math.h>

int max4(int, int, int, int);

int main()
{	

	int a, b, c, d;
	scanf("%d %d %d %d", &a, &b, &c, &d);
	

	printf("%d", max4(a,b,c,d));

	return 0;

}

int max4(int x, int y, int z, int t)
{
	int max;

	if (x > y && x > z && x > t)
	{
		return x;
	}
	
	else if (y > x && y > z && y > t)
	{
		return y;
	}
	
	else if (z > x && z > y && z > t)
	{
		return z;
	}

	else
	{
		return t;
	}
}
